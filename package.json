{
  "name": "weather-service",
  "version": "1.0.0",
  "description": "Weather API Service",
  "main": "index.js",
  "scripts": {
    "build:tsc": "tsc && tsc-alias",
    "start": "npm run build:tsc && cross-env NODE_ENV=production node dist/server.js",
    "migrate:latest": "knex --knexfile app/config/knex.ts migrate:latest",
    "migrate:rollback": "knex --knexfile app/config/knex.ts migrate:rollback",
    "knex:seeds": "knex --knexfile app/config/knex.ts seed:run",
    "start:watch": "cross-env NODE_ENV=development nodemon",
    "test": "jest --detectOpenHandles --forceExit",
    "format": "prettier --write \"app/**/*.ts\"",
    "lint": "tslint \"app/**/*.ts\"",
    "check.types": "tsc --pretty --noEmit --skipLibCheck"
  },
  "author": "enikosoft@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/knex": "^0.16.1",
    "axios": "^1.2.1",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "event-dispatch": "^0.4.1",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.14.2",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "objection": "^3.0.1",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.16.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "jest": "^29.3.1",
    "knex-cli": "^1.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "run-sequence": "^2.2.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.9.4",
    "typescript-formatter": "^7.2.2"
  }
}
